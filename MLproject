name: mlflow_pipeline

entry_points:
    main:
        parameters:
            use_dummy_data:
                type: bool
                default: false
            use_test_model:
                type: bool
                default: false
            override_hydra_conf:
                # use for change input_parameters
                type: string
                default: 'input_parameters=rain/humidity'

        command: |
            python -m main \
                use_dummy_data={use_dummy_data} \
                train.use_test_model={use_test_model} \
                {override_hydra_conf}